<h1><%= @student.dojo.branch %> <%= @student.first_name %> <%= @student.last_name %></h1>
<p>Email: <%= @student.email %> </p>

<table>
  <h1>Cohort</h1>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
  </tr>
    <% @cohort.each do |student| %>
  <tr>
    <td><%= student.first_name %></td>
    <td><%= student.last_name %></td>
    <td><%= student.email %></td>
  </tr>
  <% end %>
</table>

